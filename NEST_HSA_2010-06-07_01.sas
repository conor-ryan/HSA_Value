   IF State EQ 'NM' THEN    STAXRATE=NMTAXRATE;
   IF State EQ 'NY' THEN    STAXRATE=NYTAXRATE;
   IF State EQ 'NC' THEN    STAXRATE=NCTAXRATE;
   IF State EQ 'ND' THEN    STAXRATE=NDTAXRATE;

   IF State EQ 'OH' THEN    STAXRATE=OHTAXRATE;
   IF State EQ 'OK' THEN    STAXRATE=OKTAXRATE;
   IF State EQ 'OR' THEN    STAXRATE=ORTAXRATE;

   IF State EQ 'PA' THEN    STAXRATE=PATAXRATE;

   IF State EQ 'RI' THEN    STAXRATE=RITAXRATE;
   IF State EQ 'SC' THEN    STAXRATE=SCTAXRATE;
   IF State EQ 'SD' THEN    STAXRATE=SDTAXRATE;
   IF State EQ 'TN' THEN    STAXRATE=TNTAXRATE;
   IF State EQ 'TX' THEN    STAXRATE=TXTAXRATE;
   IF State EQ 'UT' THEN    STAXRATE=UTTAXRATE;
   IF State EQ 'VT' THEN    STAXRATE=VTTAXRATE;

   IF State EQ 'VA' THEN    STAXRATE=VATAXRATE;
   IF State EQ 'WA' THEN    STAXRATE=WATAXRATE;
   IF State EQ 'WV' THEN    STAXRATE=WVTAXRATE;
   IF State EQ 'WI' THEN    STAXRATE=WITAXRATE;
   IF State EQ 'WY' THEN    STAXRATE=WYTAXRATE;
   NEWSTATE=State;

   IF PAYCHECK2002>=84900 THEN do;
      sub_fed  =fedtaxrate;
      sub_state=staxrate;
      sub_fica =0.0145;
      marg_rate=sub_fed+sub_state+sub_fica;
   end;

   else do;
      sub_fed  =fedtaxrate;
      sub_state=staxrate;
      sub_fica =0.0765;
      marg_rate=sub_fed+sub_state+sub_fica;
   end;

   ADJPREM=NEWPREM*(1-marg_rate);

   scmedpr2=(ADJPREM/1000);

   if hmo=1 and cost>895 then ls_chronic=1;

   sclb=clb/1000;
   scub=cub/1000;
   sinc=paycheck2002/1000;
   EPO_inc =EPO*sinc;
   HMO_inc =HMO*sinc;
   HMO2_inc=HMO2*sinc;
   HRAG_inc=HRAG*sinc;
   HRAS_inc=HRAS*sinc;
   HSA_inc =HSA*sinc;
   POS_inc =POS*sinc;
   PPO_INC =PPO*SINC;
 
   sage=age/100;
   EPO_AGE =EPO*sAGE;
   HMO_AGE =HMO*sAGE;
   HMO2_AGE=HMO2*sAGE;
   HRAG_AGE=HRAG*sAGE;
   HRAS_AGE=HRAS*sAGE;
   HSA_AGE =HSA*sAGE;
   POS_AGE =POS*sAGE;
   PPO_AGE =PPO*SAGE;

   EPO_FEM =EPO*FEMALE;
   HMO_FEM =HMO*FEMALE;
   HMO2_FEM=HMO2*FEMALE;
   HRAG_FEM=HRAG*FEMALE;
   HRAS_FEM=HRAS*FEMALE;
   HSA_FEM =HSA*FEMALE;
   POS_FEM =POS*FEMALE;
   PPO_FEM =PPO*FEMALE;

   EPO_FAM =EPO*FAMILY;
   HMO_FAM =HMO*FAMILY;
   HMO2_FAM=HMO2*FAMILY;
   HRAG_FAM=HRAG*FAMILY;
   HRAS_FAM=HRAS*FAMILY;
   HSA_FAM =HSA*FAMILY;
   POS_FAM =POS*FAMILY;
   PPO_FAM =PPO*FAMILY;

   EPO_CHR =EPO*LS_CHRONIC;
   HMO_CHR =HMO*LS_CHRONIC;
   HMO2_CHR=HMO2*LS_CHRONIC;
   HRAG_CHR=HRAG*LS_CHRONIC;
   HRAS_CHR=HRAS*LS_CHRONIC;
   HSA_CHR =HSA*LS_CHRONIC;
   POS_CHR =POS*LS_CHRONIC;
   PPO_CHR =PPO*LS_CHRONIC;

   if HRAS=1 OR HRAG=1 OR HSA=1 THEN CDHPNEST=1; ELSE CDHPNEST=0;
   IF CDHPNEST=0 THEN NDHPNEST=1 ; ELSE NDHPNEST=0;

   NDHP_inc=NDHPNEST*sinc;
   CDHP_inc=CDHPNEST*sinc;

   NDHP_age=NDHPNEST*sage;
   CDHP_age=CDHPNEST*sage;

   NDHP_fam=NDHPNEST*family;
   CDHP_fam=CDHPNEST*family;
   output;


RUN;

